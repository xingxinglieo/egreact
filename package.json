{
  "private": true,
  "name": "egreact-docs",
  "version": "1.0.0",
  "scripts": {
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "prepublishOnly": "npm run build"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@types/react": "^18.0.14",
    "egreact": "^1.3.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-reconciler": "^0.28.0",
    "react-redux": "^8.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.42",
    "father-build": "^1.17.2",
    "gh-pages": "^3.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.7.3",
    "yorkie": "^2.0.0"
  }
}
